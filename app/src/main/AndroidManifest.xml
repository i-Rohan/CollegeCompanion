<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.blank.rohansharma.collegecompanion"
    android:installLocation="internalOnly" >

    <!-- uses-permission android:name="android.permission.INTERNET" / -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />

    <uses-feature android:name="android.hardware.wifi" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name=".IntroActivity"
            android:theme="@style/IntroTheme0">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"
            android:theme="@style/IntroTheme">
        </activity>
        <activity android:name=".MainActivity"
            android:theme="@style/IntroTheme">
        </activity>
        <activity
            android:name=".TimeTableActivity"
            android:label="@string/title_activity_time_table"
            android:parentActivityName=".MainActivity"
            android:theme="@style/IntroTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.blank.rohansharma.collegecompanion.MainActivity" />
        </activity>

        <receiver
            android:name=".MyReceiver"
            android:enabled="true"
            android:exported="true" >
        </receiver>

        <activity
            android:name=".AttendanceActivityS"
            android:label="@string/title_activity_attendance_activity_s"
            android:parentActivityName=".MainActivity"
            android:theme="@style/IntroTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.blank.rohansharma.collegecompanion.MainActivity" />
        </activity>

        <receiver
            android:name=".Receiver0"
            android:enabled="true"
            android:exported="true" >
            <intent-filter>
                <action android:name="abc" />
            </intent-filter>
        </receiver>
        <receiver
            android:name=".ReceiverChangeProfile"
            android:enabled="true"
            android:exported="true" >
            <intent-filter>
                <action android:name="changeProfileToNormal" />
            </intent-filter>
        </receiver>

        <activity
            android:name=".AttendanceActivityT"
            android:label="@string/title_activity_attendance_activity_t"
            android:parentActivityName=".MainActivity"
            android:theme="@style/IntroTheme" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.blank.rohansharma.collegecompanion.MainActivity" />
        </activity>
    </application>

</manifest>
